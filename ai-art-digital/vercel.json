{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/ai-art-digital/browser"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://your-azure-backend-url.com/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
    
  ]
}